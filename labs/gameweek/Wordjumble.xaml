<Page x:Class="gameweek.Wordjumble"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:gameweek"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Wordjumble">

    <Grid Width="800" Height="450">
        <MediaElement x:Name="backgroundVid" Source="jumble.mp4" LoadedBehavior="Manual" Stretch="Fill" Grid.RowSpan="2" Grid.ColumnSpan="3"/>
        <TextBox x:Name="Answertxt" Height="30" Width="200" Margin="0,347,0,72.6" Background="Aqua" TextChanged="Answertxt_TextChanged" Grid.Column="2" />
        <Label x:Name="playerturn" Content="" HorizontalAlignment="Left" Height="49" Margin="10,226,0,0" VerticalAlignment="Top" Width="193" FontSize="30" Background="Aqua" DataContextChanged="Label_DataContextChanged"/>
        <Label x:Name="Wordjumbler" Content="hello" FontSize="30" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Aqua" Foreground="Black" FontWeight="DemiBold" Margin="286,48,259,312.6"/>
       
    </Grid>

</Page>
